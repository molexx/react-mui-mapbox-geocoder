{
  "name": "react-mui-mapbox-geocoder",
  "version": "2.2.0",
  "description": "A Material UI Autosuggest'ing Mapbox Geocoder for locating addresses and points of interest.",
  "repository": {
    "type": "git",
    "url": "https://github.com/wheredoesyourmindgo/react-mui-mapbox-geocoder"
  },
  "keywords": [
    "geocoding",
    "geocoder",
    "address",
    "mapbox",
    "material-ui",
    "react",
    "autosuggest"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "source": "src/index.ts",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "postpublish": "git push",
    "type-check": "tsc -p .",
    "install-peers": "install-peers -f",
    "build": "microbundle --jsx React.createElement --tsconfig tsconfig.json --jsxFragment React.Fragment",
    "dev": "yarn build watch",
    "clean": "rimraf {dist,.rts2*}"
  },
  "author": "Abe Hendricks<moemanchu@icloud.com>",
  "devDependencies": {
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/react": "^17.0.27",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-dom": "^17.0.9",
    "install-peers-cli": "^2.2.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.3.4",
    "react-autosuggest": "^10.1.0",
    "use-debounce": "^8.0.3"
  }
}
